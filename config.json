{
	"ports": {

		"4080" : {
			"redirect": {
				"*.code2flow.com/^(?<code>[a-f]{6})" : "http://code2flow.com/[code]",
				"*.net/*" : "https://[1]/[2]",
			},
			"errorHtmlPage": "/home/rush/Programowanie/rush-http-proxy/test.html",
			"proxy": {
				"local.code2flow.com": 80,
				"*" : "4000"
			},
			"static": {}
		}
	},
  "logging": {
    "logFile": "/tmp/access.log",
    "appLog": "/tmp/app.log"
  }
}